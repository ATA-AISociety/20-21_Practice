print("Input a number to check if it is a factor of another number")

firsnum = input("first number: ")

secNum = input("Second number:  ")


if(int(firsnum) % int(secNum)) != 0 :

    print(str(firsnum) + " is not a factor of " + str(secNum))
else :
    print(" They are fators")
