# Fibonacci Sequence
def big_ifs():
    x = input("Enter a number: ")
    x = int(x)
    if x > 1:
        for i in range(2, x):
            if (x % i) == 0:
                print(x, "is not a prime number.")
                break
        else:
            print(x, "is a prime number.")
    else:
        print(x, "is not a prime number.")

x = input("Enter in X's number: ")
y = input("Enter in Y's number: ")
if "x" > "y":
  print("Y looses to X")
if "y" > "x":
  print(" X looses to Y")
  
